{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Fatal",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%PROGRAMDATA%/Osmo/logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "http://*:5005"
      }
    }
  },
  "ConneXOptions": {
    "ConneXHost": {
      "HostName": "connexion.dataio.internal"
    }
  },
  "PostgresOptions": {
    "Host": "localhost",
    "Username": "dataio",
    "Password": "dataio",
    "Port": 5432
  }
}
