{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Fatal",
        "System.Net.Http.HttpClient": "Warning"
      }
    }
  },

  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "http://*:5005"
      }
    }
  },
  "ConneXOptions": {
    "ConneXHost": {
      "HostName": "connexion.dataio.internal"
    }
  }
}
